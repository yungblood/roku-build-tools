<?xml version="1.0" encoding="utf-8" ?>
<component name="FocusGroup" extends="Group">
	<script type="text/brightscript" uri="pkg:/components/framework/widgets/focusGroup.brs" />
	<interface>
        <field id="advanceKey" type="string" value="right" />
        <field id="reverseKey" type="string" value="left" />

        <field id="itemFocused" type="integer" value="-1" alwaysNotify="true" />
        <field id="itemUnfocused" type="integer" value="-1" alwaysNotify="true" />
        <field id="jumpToIndex" type="integer" onChange="jumpToIndex" alwaysNotify="true" />
        
        <field id="wrap" type="boolean" />
	</interface>
</component>
    